{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*":[
        "./src/components/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
      "@lib*":[
        "./src/lib/*"
      ],
      "@dtos*":[
        "./src/dtos/*"
      ],
    }
  }
}